<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://www.psaidoesnt-exist.org/psaiXmlMessages" elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:psai="http://www.psaidoesnt-exist.org/psaiXmlMessages"
>
	
	<!-- Global types -->
	
	<!-- 3D Vector -->
	<complexType name="vector3d">
		<sequence>
			<element name="x" type="float" minOccurs="1" maxOccurs="1" />
			<element name="y" type="float" minOccurs="1" maxOccurs="1" />
			<element name="z" type="float" minOccurs="1" maxOccurs="1" />
		</sequence>
	</complexType>
	
	<!-- Container Contents -->
	<complexType name="containercontents">
		<all>
			<element name="name" type="string" minOccurs="1" maxOccurs="1" />
			<element name="icon" type="string" minOccurs="1" maxOccurs="1" />
			<element name="slotid" type="int" minOccurs="1" maxOccurs="1" />
			<element name="stackCount" type="int" minOccurs="1" maxOccurs="1" />
			<element name="purifystatus" type="int" minOccurs="1" maxOccurs="1" />
		</all>
	</complexType>
	
	<!-- Item Description in GUI Inventory Message-->
	<complexType name="itemdescription">
		<all>
			<element name="name" type="string" minOccurs="1" maxOccurs="1" />
			<element name="slot" type="int" minOccurs="1" maxOccurs="1" />
			<element name="weight" type="float" minOccurs="1" maxOccurs="1" />
			<element name="size" type="float" minOccurs="1" maxOccurs="1" />
			<element name="stackcount" type="int" minOccurs="1" maxOccurs="1" />
			<element name="iconimage" type="string" minOccurs="1" maxOccurs="1" />
			<element name="container" type="int" minOccurs="1" maxOccurs="1" />
			<element name="purifystatus" type="int" minOccurs="1" maxOccurs="1" />
		</all>
	</complexType>
	
	<!--Skills details  -->
	<complexType name="networkdetailskill">
		<all>
			<element name="category" type="int" minOccurs="1" maxOccurs="1" />
			<element name="text" type="string" minOccurs="1" maxOccurs="1" />
		</all>
	</complexType>
	
	<!-- Network weather info -->
	<complexType name="netweatherinfo">
		<all>
			<element name="hasdownfall" type="boolean" minOccurs="1" maxOccurs="1" />
			<element name="downfallissnow" type="boolean" minOccurs="1" maxOccurs="1" />
			<element name="hasfog" type="boolean" minOccurs="1" maxOccurs="1" />
			<element name="haslightning" type="boolean" minOccurs="1" maxOccurs="1" />
			<element name="sector" type="string" minOccurs="1" maxOccurs="1" />
			<element name="downfalldrops" type="int" minOccurs="1" maxOccurs="1" />
			<element name="downfallfade" type="int" minOccurs="1" maxOccurs="1" />
			<element name="fogdensity" type="int" minOccurs="1" maxOccurs="1" />
			<element name="fogfade" type="int" minOccurs="1" maxOccurs="1" />
			<element name="fogred" type="int" minOccurs="1" maxOccurs="1" />
			<element name="foggreen" type="int" minOccurs="1" maxOccurs="1" />
			<element name="fogblue" type="int" minOccurs="1" maxOccurs="1" />
		</all>
	</complexType>
	
	<!-- Money object -->
	<complexType name="psmoney">
		<all minOccurs="1" maxOccurs="1">
			<element name="circles" type="int" minOccurs="1" maxOccurs="1" />
			<element name="octas" type="int" minOccurs="1" maxOccurs="1" />
			<element name="hexas" type="int" minOccurs="1" maxOccurs="1" />
			<element name="trias" type="int" minOccurs="1" maxOccurs="1" />
		</all>
	</complexType>

