SubDir TOP tests xml ;

# specify the library to be built
Library psaixmltests
	: [ Wildcard *.cpp *.h ]
	: noinstall
;

# specify the required libs for the build
ExternalLibs psaixmltests : CPPUNIT ;
# CRYSTAL CEL PSUTIL PSENGINE CPPUNIT ;

# specify compile group membership
CompileGroups psaixmltests : tests ;